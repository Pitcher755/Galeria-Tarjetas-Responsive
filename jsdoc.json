{
    "source": {
        "include": [
            "js"
        ],
        "exclude": [
            "js/utils",
            "node_modules"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "readme": "./README.md",
        "template": "node_modules/minami"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}